{
	"name": "Offbase",
	"type": "dark",
	"colors": {
		"activityBar.background": "#444444",
		"activityBar.border": "#2F8FBF",
		"activityBar.foreground": "#888888",
		"activityBarBadge.foreground": "#FFFFFF",
		"activityBarBadge.background": "#2F8FBF",
		"editor.background": "#222222",
		"editor.foreground": "#AAAAAA",
		"editorError.border": "#BF2F2F",
		"editorLineNumber.foreground": "#444444",
		"notification.background": "#444444",
		"notification.foreground": "#AAAAAA",
		"panel.border": "#2F8FBF",
		"panelTitle.activeBorder": "#2F8FBF",
		"panelTitle.activeForeground": "#2F8FBF",
		"panelTitle.inactiveForeground": "#444444",
		"sideBar.background": "#222222",
		"sideBar.border": "#888888",
		"sideBar.foreground": "#AAAAAA",
		"sideBarSectionHeader.background": "#444444",
		"sideBarSectionHeader.foreground": "#000000",
		"sideBarTitle.foreground": "#AAAAAA",
		"terminal.ansiBlue": "#2F8FBF",
		"terminal.ansiCyan": "#2FBF8F",
		"terminal.ansiGreen": "#8FBF2F",
		"terminal.ansiMagenta": "#8F2FBF",
		"terminal.ansiRed": "#BF2F2F",
		"terminal.ansiYellow": "#BF8F2F",
		"titleBar.border": "#2F8FBF"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment"],
			"settings": {
				"foreground": "#444444"
			}
		},
		{
			"name": "Constant",
			"scope": ["constant"],
			"settings":{
				"foreground": "#AFAF2F"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"foreground": "#BF2F2F"
			}
		},
		{
			"name": "Entity, Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#2FBF8F"
			}
		},
		{
			"name": "Entity, Section",
			"scope": ["entity.name.section"],
			"settings":{
				"foreground": "#AAAAAA",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Entity, Selector",
			"scope": ["entity.name.selector"],
			"settings":{
				"foreground": "#AAAAAA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword"],
			"settings": {
				"foreground": "#AAAAAA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword, Control",
			"scope": ["keyword.control"],
			"settings":{
				"foreground": "#AAAAAA",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Keyword, Control, Directive",
			"scope": ["keyword.control.directive"],
			"settings":{
				"foreground": "#8F2FBF",
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation"],
			"settings":{
				"foreground": "#666666"
			}
		},
		{
			"name": "Punctuation, String",
			"scope": ["punctuation.definition.string"],
			"settings":{
				"foreground": "#BF8F2F",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#AF6F2F"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#2F8FBF"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#8FBF2F"
			}
		}
	]
}